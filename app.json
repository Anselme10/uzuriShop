{
  "expo": {
    "name": "Uzuri Shop",
    "slug": "Uzuri-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uzurishop",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/uzlogotrans.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFECF7"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anselmeoga.Uzurishop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/uzuri1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anselmeoga.Uzurishop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/uzlogotrans.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "128f44ee-dd31-40c4-9559-63648a0aa042"
      }
    }
  }
}
